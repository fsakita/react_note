{
  "name": "react_note",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "concurrently \"NODE_ENV=development webpack-dev-server --config webpack.conf/dev.config.js & open http://localhost:8080/\"",
    "build": "NODE_ENV=production webpack --config webpack.conf/prod.config.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^actions": "<rootDir>/src/actions",
      "^actionTypes": "<rootDir>/src/actionTypes",
      "^strings": "<rootDir>/src/config/strings.js",
      "^services": "<rootDir>/src/services",
      "^stores": "<rootDir>/src/stores",
      "^utils": "<rootDir>/src/utils",
      "components(.*)$": "<rootDir>/src/components/$1",
      "mocks(.*)$": "<rootDir>/__mocks__/$1"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/setupTestFrameworkScriptFile.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "xlsx": "^0.14.0"
  },
  "dependencies": {
    "eventemitter3": "^3.1.0",
    "flux": "^3.1.3",
    "keymirror": "^0.1.1",
    "react": "^16.5.2",
    "react-data-export": "^0.5.0",
    "react-dom": "^16.5.2",
    "react-onevent": "^1.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.82.5"
  }
